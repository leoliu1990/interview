package edu.rice.myqueue;

import java.util.Stack;

public class MyQueue<E> {
	private Stack<E> in;
	private Stack<E> out;
	private int count;
	
	public MyQueue(){
		in = new Stack<E>();
		out = new Stack<E>();
		count = 0;
	}
	
	public E add(E item){
		in.push(item);
		count++;
		return item;
	}
	
	public E peek(){
		if(out.empty())
			return null;
		
	}
}
