package edu.rice.tree;

public class BinaryTree {
	private Node root;
	
	public BinaryTree(){
		this.setRoot(null);
	}

	public Node getRoot() {
		return root;
	}

	public void setRoot(Node root) {
		this.root = root;
	}
	
	public String toString(){
		return null;
		
	}
	
	private String serialize(Node node, String s){
		if(node == null){
			String r = s+'#';
			return r;
		}
		String r = s + node.getData();
		r += serialize(node.getLeft(), r);
		r += serialize(node.getRight(), r);
		
		return r;
	}
}
