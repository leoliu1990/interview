package edu.rice.replspace;

public class ReplSpace {
	public static void main(String s[]){
		
	}
	
	private char[] replSpace(char a[], int length){
		StringBuffer sb = new StringBuffer();
		int len = a.length;
		for(int i = 0; i < len; i++){
			if(a[i] == ' ')
				sb.append("%20");
			else
				sb.append(a[i]);
		}
		return sb.toString().toCharArray();
	}
}
