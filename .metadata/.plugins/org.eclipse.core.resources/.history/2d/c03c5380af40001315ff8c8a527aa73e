package edu.rice.hanoi;

import java.util.Stack;

public class Tower {
	private Stack<Integer> disks;
	
	public Tower(){
		disks = new Stack<Integer>();
	}
	
	public int add(int t){
		if(!disks.empty() && disks.peek() <= t){
			System.out.println("Error in adding disk");
			return -1;
		}else{
			disks.push(t);
			return t;
		}
	}
}
